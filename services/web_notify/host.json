{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Host": "Warning"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.3.0, 4.0.0)"
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "main"
    },
    "enableForwardingHttpRequest": true
  },
  "extensions": {
    "queues": {
      "maxDequeueCount": 2,
      "messageEncoding": "base64"
    }
  }
}
