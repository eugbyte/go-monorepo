{
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "request",
      "methods": [
        "post"
      ],
      "route": "notify-queue"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "response"
    },
    {
      "type": "queue",
      "name": "message",
      "direction": "out",
      "queueName": "web-notifications",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
